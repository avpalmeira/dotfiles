! Let CAPSLOCK work as ESC
remove Lock = Caps_Lock
keysym 0x42 = Escape

! Maps german characters to more useful chars
! u and U  umlaut
keysym 0xfc = quotedbl
keysym 0xdc = bar
! o and O umlaut
keysym 0xf6 = braceleft
keysym 0xd6 = bracketleft
! a and A umlaut
keysym 0xe4 = braceright
keysym 0xc4 = bracketright
! map ssharp
keysym 0xdf = backslash
! map double quotes
keysym 0x22 = at
